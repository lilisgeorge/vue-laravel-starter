{
  "name": "vue-laravel-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -rf ../api/public/{app} && vue-cli-service build --no-clean",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
